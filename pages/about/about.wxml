<!--pages/about/about.wxml-->
<view class="container" bindtouchstart="scrollTouchstart" bindtouchmove="scrollTouchmove">
  <view class="userinfo">
    <block>
      <image bindtap="tapImg" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </block>
  </view>
</view>
